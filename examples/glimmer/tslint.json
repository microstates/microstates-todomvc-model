{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "only-arrow-functions": false,
    "prefer-const": false,
    "member-access": [true, "no-public"],
    "semicolon": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
